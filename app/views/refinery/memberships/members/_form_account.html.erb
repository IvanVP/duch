<div id="contact1">
   
  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <%= f.label :username, t('refinery.members.shared.username') %>
    <%= f.text_field :username, :class => "form-control" %>
  </div>

  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <%= f.label :first_name, t('refinery.members.shared.first_name') %>
    <%= f.text_field :first_name, :class => "form-control" %>
  </div>

  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <%= f.label :last_name, t('refinery.members.shared.last_name') %>
    <%= f.text_field :last_name, :class => "form-control" %>
  </div>

  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <%= f.label :email, t('refinery.members.shared.email') %>
    <%= f.email_field :email, :class => "form-control" %>
  </div>

  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <%= f.label :password, t('refinery.members.shared.password') %>
    <%= f.password_field :password, :class => "form-control", :autocomplete => 'off' %>
  </div>
  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <%= f.label :password_confirmation, t('refinery.members.shared.password_confirmation') %>
    <%= f.password_field :password_confirmation, :class => "form-control", :autocomplete => 'off' %>
    <%= "<p class='nonpassword'>#{t('members.form.blank_password_keeps_current', :scope => 'refinery.memberships')}</p>".html_safe if @member.persisted? %>
  </div>
</div>

